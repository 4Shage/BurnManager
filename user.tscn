script/source = "extends Panel
var user : Dictionary = {
		\"name\":\"Paweł Harasymowicz\",
		\"cost\":\"60\",
		\"borrowed\":\"0\",
		\"startDate\":\"01.06\",
		\"endDate\":\"01.09\",
		\"dayOn\":{
			1:0,
			3:0
		},
		\"dayOff\":{
			\"12.08\":0
		},
		\"wild\":0
	}

@onready var line_cost = $VBoxContainer/BoxContainer/LineEdit
@onready var line_borrowed = $VBoxContainer/BoxContainer4/LineEdit
@onready var line_weekdays = $VBoxContainer/BoxContainer2/LineEdit
@onready var line_sn = $HBoxContainer/LineEdit
@onready var line_n = $HBoxContainer/LineEdit2
@onready var il = $VBoxContainer/BoxContainer3/ItemList
@onready var line_dOff = $VBoxContainer/BoxContainer3/BoxContainer/LineEdit





func _ready():
	if Manager.users != {}:
		user = Manager.users[Manager.selUser]
		line_sn.text = Manager.selUser
	line_n.text = user[\"name\"]
	line_cost.text = user[\"cost\"]
	line_borrowed.text = user[\"borrowed\"]
	var weekdays
	for e in user[\"dayOn\"]:
		var i : String
		match e:
			0:
				i = \"1\"
			1:
				i = \"2\"
			2:
				i = \"3\"
			3:
				i = \"4\"
			4:
				i = \"5\"
			5:
				i = \"6\"
			6:
				i = \"7\"
		if weekdays != null:
				weekdays = weekdays + \",\"+i
		else:
			weekdays = i
			
	line_weekdays.text = weekdays
	for e in user[\"dayOff\"]:
		il.add_item(e)


func _on_button_pressed():
	if line_dOff.text == \"\":
		return
	user[\"dayOff\"][line_dOff.text] = 0
	il.clear()
	for e in user[\"dayOff\"]:
		il.add_item(e)
"

