[sub_resource type="GDScript" id="GDScript_uaa3h"]
script/source = "extends Button

var FB : FirebaseAuth = Firebase.Auth
var m : bool

func _ready() -> void:
	if Data.auth == null:
		self.text = \"Login\"
		m = false
	else:
		self.text = \"Log out\"
		m = true

func _on_pressed() -> void:
	#Manager.saveSelf()
	if m:
		FB.logout()
		Data.auth = null
	else:
		FB.logout()
		Data.auth = null
		Manager.chScene(\"auth\")
"

